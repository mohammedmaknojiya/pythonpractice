l=[-2,1,-3,4,-1,2,1,-5,4]
k=[]
j=0

while(j<len(l)):

    for i in range(1+j,len(l)+1):
        su=0
        su=su+sum(l[j:i])
        k.append(su)

    j+=1
lar=max(k)
print(k)
print(lar)

output:


[-2, -1, -4, 0, -1, 1, 2, -3, 1, 1, -2, 2, 1, 3, 4, -1, 3, -3, 1, 0, 2, 3, -2, 2, 4, 3, 5, 6, 1, 5, -1, 1, 2, -3, 1, 2, 3, -2, 2, 1, -4, 0, -5, -1, 4]
6

